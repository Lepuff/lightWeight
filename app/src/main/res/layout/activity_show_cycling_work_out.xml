<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.CyclingWorkOut.ShowCyclingWorkOut">

    <TextView
        android:id="@+id/text_matning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/timing"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_timing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textedit_timing" />

    <EditText
        android:id="@+id/textedit_timing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_top_speed" />

    <TextView
        android:id="@+id/text_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_total"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_timing" />

    <EditText
        android:id="@+id/textedit_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_timing" />

    <TextView
        android:id="@+id/text_total_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total_time"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_total_time"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_total" />

    <EditText
        android:id="@+id/textedit_total_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_total" />

    <TextView
        android:id="@+id/text_pulse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/average_pulse"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_average_pulse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_total_time" />

    <EditText
        android:id="@+id/textedit_average_pulse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_total_time" />

    <TextView
        android:id="@+id/text_top_pulse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Max pulse"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_max_pulse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_pulse" />

    <EditText
        android:id="@+id/textedit_max_pulse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_pulse" />

    <TextView
        android:id="@+id/text_average_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/average_speed"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_average_speed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textedit_average_speed" />

    <EditText
        android:id="@+id/textedit_average_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_top_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/top_speed"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_top_speed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textedit_top_speed" />

    <EditText
        android:id="@+id/textedit_top_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_speed" />

    <TextView
        android:id="@+id/text_fitness_exercise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fitness_exercise"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_fitness_exercise"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_max_pulse" />

    <EditText
        android:id="@+id/textedit_fitness_exercise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_max_pulse" />

    <TextView
        android:id="@+id/text_anaerobic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Anaerobic"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_anaerobic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_fitness_exercise" />

    <EditText
        android:id="@+id/textedit_anaerobic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_fitness_exercise" />

    <TextView
        android:id="@+id/text_average_force"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/average_force"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_average_force"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_anaerobic" />

    <EditText
        android:id="@+id/textedit_average_force"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_anaerobic" />

    <TextView
        android:id="@+id/text_average_cadence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/average_cadence"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_average_cadence"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_force" />

    <EditText
        android:id="@+id/textedit_average_cadence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_force" />

    <TextView
        android:id="@+id/text_top_cadence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/top_cadence"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_top_cadence"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_cadence" />

    <EditText
        android:id="@+id/textedit_top_cadence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_average_cadence" />

    <TextView
        android:id="@+id/text_calories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/calories"
        app:layout_constraintBottom_toBottomOf="@+id/textedit_calories"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_top_cadence" />

    <EditText
        android:id="@+id/textedit_calories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textedit_top_cadence" />

</androidx.constraintlayout.widget.ConstraintLayout>